{
  "name": "minimal-consent",
  "author": "Gerald Madlmayr <gerald.madlmayr@gmail.com>",
  "license": "SEE LICENSE IN ./LICENSE.txt",
  "private": true,
  "engines": {
    "node": ">=0.8.0"
  },
  "scripts": {
    "lint": "eslint . --ext .js",
    "clean": "rm -rf ./dist/",
    "start": "rm -rf ./dist/; NODE_ENV=development webpack --watch",
    "build": "rm -rf ./dist/; NODE_ENV=production webpack",
    "test": "nyc mocha --require @babel/register --recursive --reporter list",
    "format": "prettier --write \"{src,client,typings,specs}/**/*.ts\""
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/register": "^7.0.0",
    "chai": "^4.2.0",
    "copy-webpack-plugin": "5.0.5",
    "css-loader": "3.2.0",
    "eslint": "^6.6.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "4.3.0",
    "fs": "0.0.1-security",
    "fs-extra": "8.1.0",
    "html-loader": "0.5.5",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "sinon-chrome": "^3.0.1",
    "style-loader": "1.0.0",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.10",
    "webpack-extension-reloader": "^1.1.4"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "browser": true
    },
    "globals": {
      "chrome": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 8
    },
    "rules": {
      "eol-last": 0,
      "quotes": [
        2,
        "single"
      ]
    }
  },
  "dependencies": {
    "jquery": "^3.4.1",
    "js-logger": "^1.6.0",
    "dateformat": "^3.0.3"
  }
}
