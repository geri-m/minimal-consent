{
  "name": "Minimal Consent Chrome extension",
  "version": "1.0",
  "description": "This Chrome extensions give minimal consent to webpages and stores only the minimum amount of cookies. .",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "./background.bundle.js"
    ]
  },
  "icons": {
    "16": "./images/Icon_16x16.png",
    "32": "./images/Icon_32x32.png",
    "48": "./images/Icon_48x48.png",
    "128": "./images/Icon_128x128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["./contentscript.bundle.js"],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "browser_action": {
    "default_icon": {
      "16": "./images/Icon_16x16.png",
      "32": "./images/Icon_32x32.png",
      "48": "./images/Icon_48x48.png",
      "128": "./images/Icon_128x128.png"
    },
    "default_title": "Minimal Consent"
  }
}


